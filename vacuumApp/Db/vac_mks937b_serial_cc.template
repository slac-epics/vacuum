# Cold Cathode Enable
# 
# % archiver 10 Monitor
# % controldesk Pressure scon
record(bo, "$(device):STATEDES") {
  field(DESC, "Cold Cathode Enable")
  field(DTYP, "stream")
  field(OUT, "@mks937b.proto.db ccenable($(channel),$(addr)) $(port) 0")
#field(FLNK, "$(device):STATE")
  field(ZNAM, "OFF")
  field(ONAM, "ON")
