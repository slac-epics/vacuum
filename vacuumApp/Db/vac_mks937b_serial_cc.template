  field(FVST, "RP_OFF")
  field(SXST, "Wait")
  field(SVST, "LowEmis")
  field(EIST, "CNTRL_Off")
  field(NIST, "Protected State")
  field(TEST, "MISCONN")
  field(ELST, "NoGauge")
  field(TVST, "Bad Command")
  field(TTST, "")
  field(FTST, "")
  field(FFST, "")
#  field(UNSV, "Locked Out")
  field(ZRSV, "NO_ALARM")
  field(ONSV, "NO_ALARM")
  field(TWSV, "NO_ALARM")
  field(THSV, "MINOR")
  field(FRSV, "MAJOR")
  field(FVSV, "MAJOR")
  field(SXSV, "MINOR")
  field(SVSV, "MINOR")
  field(EISV, "MAJOR")
  field(NISV, "MAJOR")
  field(TESV, "MAJOR")
  field(ELSV, "MINOR")
  field(TVSV, "MAJOR")
  field(TTSV, "MAJOR")
  field(FTSV, "MAJOR")
  field(FFSV, "MAJOR")
}

record(sel, "$(device):PMON") {
  field(DESC, "Pressure")
  field(SELM, "Specified")
  field(NVL, "$(device):STATUS_RSCALC")
  field(INPA, "$(device):PMONRAW PP MS")
  field(INPB, "1.00")
  field(FLNK, "$(device):PLOG MS")
  field(PREC, "3")
  field(EGU, "Torr")
  field(HIGH, "9.9e-3" )
  field(HSV,  "MINOR" )
  field(HIHI, "0.99" )
  field(HHSV, "MAJOR" )
}

record(ai, "$(device):PMONRAW") {
  field(SCAN, "Event")
  field(EVNT, "$(fastEvt)")
  field(DESC, "Pressure")
  field(DTYP, "stream")
  field(INP, "@mks937b.proto.db pressure($(channel),$(addr),$(device):STATUS_RS) $(port) 0")
  field(EGU, "Torr")
  field(PREC, "3")
  field(HOPR, "0.01")
  field(LOPR, "0")
  field(FLNK, "$(device):STATUS_RSCALC")
}

record(stringin, "$(device):STATUS_RS") {
  field(DESC, "Channel Status")
  field(PINI, "YES")
  field(VAL, "Ok")
}


# Clean up Combo Pressure Status if there is no error
record(calcout, "$(device):STATUS_RS_CALC1") {
  field(INPA, "$(device):PMONRAW.SEVR CP")
  field(CALC, "A#2")
  field(OOPT, "When Non-zero")
  field(OUT, "$(device):STATUS_RS_CALC2.PROC")
}

record(stringout, "$(device):STATUS_RS_CALC2") {
  field(VAL, "OK")
  field(OUT, "$(device):STATUS_RS PP")
}

